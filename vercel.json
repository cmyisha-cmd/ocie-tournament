{
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "register.html", "use": "@vercel/static" },
    { "src": "players.html", "use": "@vercel/static" },
    { "src": "admin.html", "use": "@vercel/static" },
    { "src": "dashboard.html", "use": "@vercel/static" },
    { "src": "public/**/*", "use": "@vercel/static" }
